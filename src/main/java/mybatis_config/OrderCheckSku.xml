<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	<mapper namespace="com.what21.dao.OrderCheckSkuDao">
	<resultMap type="OrderCheckSku" id="OrderCheckSkuResultMap">
		<id property="id" column="id"/>
		<result property="totalid" column="totalid"/>
		<result property="totalMailNo" column="totalMailNo"/>
		<result property="billcode" column="billcode"/>
		<result property="createdatatime" column="createdatatime"/>
		<result property="status" column="status"/>
		<result property="weight" column="weight"/>
		
	</resultMap>
	
	<select id="findBySku" resultMap="OrderCheckSkuResultMap" parameterType="java.util.Map">
		select *  from cz_ordercheck_sku  where totalMailNo = #{totalMailNo} and totalid = #{totalid} 
		order by  id desc 
		limit ${startPage},${endPage}
	</select>
	
	<select id="count" resultType="java.lang.Integer" parameterType="java.util.Map">
		select count(1)  from cz_ordercheck_sku  where totalMailNo = #{totalMailNo} and totalid = #{totalid}
	</select>
	
	<update id="add" parameterType="OrderCheckSku">
		insert into cz_ordercheck_sku (billcode,status,totalMailNo,totalid,createdatatime,weight)
		values(#{billcode},#{status},#{totalMailNo},#{totalid},#{createdatatime},#{weight})
	</update>
	
	<select id="findId" resultType="OrderCheckSku" parameterType="java.util.Map">
		select *  from cz_ordercheck_sku  where totalMailNo = #{totalMailNo} and totalid = #{totalid} and billcode = #{billcode}
	</select>
	<select id="findAll" resultMap="OrderCheckSkuResultMap" parameterType="String">
		select billcode,weight from cz_ordercheck_sku where totalMailNo = #{totidaa} and totalId = #{totAa}
	</select>
	<select id="findByBillcode" resultMap="OrderCheckSkuResultMap" parameterType="String">
		select billcode,weight from cz_ordercheck_sku where billcode = #{billcode}
	</select>
	
	</mapper>