<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.what21.dao.GoodsAcceptDao">
	<resultMap type="GoodsAccept" id="goodsResultMap">
		<id property="id" column="id"/>
		<result property="orderId" column="orderId"/>
		<result property="expressNumber" column="expressNumber"/>
		<result property="goodsName" column="goodsName"/>
		<result property="goodsType" column="goodsType"/>
		<result property="weight" column="weight"/>
		<result property="price" column="price"/>
		<result property="num" column="num"/>
		<result property="totalPrice" column="totalPrice"/>
		<result property="goodsSpec" column="goodsSpec"/>
		<result property="goodsUnit" column="goodsUnit"/>
		<result property="goodsDutyNumber" column="goodsDutyNumber"/>
		<result property="goodsHS" column="goodsHS"/>
		<result property="fromCountryCode" column="fromCountryCode"/>
		<result property="goodsSKU" column="goodsSKU"/>
		<result property="userId" column="userId"/>
	</resultMap>
	<insert id="insert" parameterType="GoodsAccept">
		INSERT INTO t_goods_accept (orderId, expressNumber, goodsName, goodsType, 
		weight, price, num, totalPrice, goodsSpec, goodsUnit, goodsDutyNumber, 
		goodsHS, fromCountryCode, goodsSKU, userId) 
		VALUES (#{orderId}, #{expressNumber}, #{goodsName}, #{goodsType}, 
		#{weight}, #{price}, #{num}, #{totalPrice}, #{goodsSpec}, 
		#{goodsUnit}, #{goodsDutyNumber}, #{goodsHS}, #{fromCountryCode}, 
		#{goodsSKU}, #{userId})
	</insert>
	
	<select id="findByExpressNumber" resultMap="goodsResultMap" parameterType="String">
		select * from t_goods_accept where expressNumber = #{expressNumber}
	</select>
	
	<delete id="delete" parameterType="String">
		DELETE FROM t_goods_accept 
		where expressNumber in 
		<foreach collection="array" item="expressNumbers" open="(" separator="," close=")">
			#{expressNumbers}
		</foreach>
	</delete>
</mapper>