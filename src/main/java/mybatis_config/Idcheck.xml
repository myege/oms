<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.what21.dao.IdcheckDao">
	<resultMap type="Idcheck" id="ItemResultMap">
		<id property="id" column="id"/>
		<result property="name" column="name"/>
		<result property="idCard" column="idCard"/>
		<result property="back" column="back"/>
		<result property="status" column="status"/>
		<result property="fromto" column="fromto"/>
		<result property="createDateTime" column="createDateTime"/>
	</resultMap>
	

	<insert id="insertBatch" >
		insert into idcheck (name,idCard,createDateTime,back,fromto,status )
		values
		<foreach collection="list" item="Idcheck" separator=",">		
		(#{Idcheck.name},#{Idcheck.idCard},#{Idcheck.createDateTime},#{Idcheck.back},
		#{Idcheck.fromto} ,#{Idcheck.status})
		</foreach>
	</insert>

	
	<select id="findgjz" resultMap="ItemResultMap" parameterType="java.util.Map">
			SELECT * from idcheck where ${gjz} 

	</select>
	
	
</mapper>