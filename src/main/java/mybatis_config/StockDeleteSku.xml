<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.what21.dao.StockDeleteSkuDao">
	<resultMap type="StockDeleteSku" id="StockDeleteSkuResultMap">
		<id property="id" column="id"/>
		<result property="stockDeleteId" column="stockDeleteId"/>
		<result property="inOutSeq" column="inOutSeq"/>
		<result property="datatime" column="datatime"/>
		<result property="flag" column="flag"/>
		<result property="userId" column="userId"/>
	</resultMap>
	
	
	<select id="findAll" resultMap="StockDeleteSkuResultMap" parameterType="java.util.Map">
		select * from wms_stock_delete_sku where 1=1 and  stockDeleteId=#{stockDeleteId}
		
		<if test="userId != null and userId != 0">
	 		and userId = #{userId} 
	    </if>
		
		LIMIT #{startPage},#{endPage}
	</select>
	
	<select id="count" resultType="java.lang.Integer">
		select count(1) from wms_stock_delete_sku
		<where>
			stockDeleteId=#{stockDeleteId}
		</where>
	</select>
	
	<insert id="insert"  parameterType="StockDelete">
	     insert into wms_stock_delete_sku(stockDeleteId,inOutSeq,datatime,flag,userId)
	     values(#{stockDeleteId},#{inOutSeq},#{datatime},#{flag},#{userId})
	</insert>
	
	<select id="findByStockDeleteId" resultMap="StockDeleteSkuResultMap" >
	
		select * from wms_stock_delete_sku where 1=1 and stockDeleteId=#{stockDeleteId}
		
	</select>
	
	</mapper>